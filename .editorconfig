# UNIX-Style
[*]
end_of_line = lf
insert_final_newline = true

[*.go]
indent_style = space
indent_size = 4

[*.{js,ts,jsx,tsx}]
indent_style = space
indent_size = 4

[*.svelte]
indent_style = space
indent_size = 2

[*.{scss,css}]
indent_style = space
indent_size = 2

[*.Makefile]
indent_style = tab




